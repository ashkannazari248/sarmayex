{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "types": [
      "@nuxt/types",
      "nuxt-svgo",
      "vite/client",
      "vue-tsc"
    ],
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "experimentalDecorators": true,
    "strictPropertyInitialization":false,
    "noImplicitAny":false,
    "removeComments": true
  }
}
